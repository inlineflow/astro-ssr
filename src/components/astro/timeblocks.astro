---
interface Props {
  openingTime: string;
  closingTime: string;
  durationInMinutes: number;
}
import { TimeBlocks } from "../react/Timeblocks";
const { openingTime, closingTime, durationInMinutes } = Astro.props;
---

<TimeBlocks
  client:load
  openingTime={openingTime}
  closingTime={closingTime}
  durationInMinutes={durationInMinutes}
/>
