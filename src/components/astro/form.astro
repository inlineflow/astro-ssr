---
import { AppointmentForm } from "../react/AppointmentForm";
import type { EstablishmentValidated } from "root/src/lib/types";

interface Props {
  validEstablishment: EstablishmentValidated;
}

const { validEstablishment } = Astro.props;
---

{
  validEstablishment && (
    <AppointmentForm service={validEstablishment} client:load />
  )
}
